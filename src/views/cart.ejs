                        <input type="text" class="input-quntity" value="1">
                        <button  class= "fld plus-btn" type= "button">+</button>
                            </div>
                    </div>
                    <a style="text-decoration: none;color: inherit;" href='/cart/<%= ele._id%>' id="remove-product">
                        
